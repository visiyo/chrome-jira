{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Chrome JIRA",
  "description": "Copy JIRA ticket to clipboard",
  "icons": { "16": "images/calculator-16.png", "128": "images/calculator-128.png" },
  "browser_action": {
    "default_icon": "images/calculator-128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}